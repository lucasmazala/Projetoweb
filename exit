[32m+[m[32mpackage br.com.projetoweb.bean;[m
[32m+[m
[32m+[m[32mimport javax.json.Json;[m
[32m+[m[32mimport javax.json.JsonObject;[m
[32m+[m
[32m+[m[32mimport com.google.gson.Gson;[m
[32m+[m
[32m+[m[32mpublic class GeoJsonBEAN {[m
[32m+[m
[32m+[m	[32mpublic void createJson(){[m
[32m+[m[41m	[m
[32m+[m	[32mJsonObject creatingJson = Json.createObjectBuilder() //cria objeto {}[m
[32m+[m			[32m.add("type","FeatureCollection")[m
[32m+[m			[32m.add("features",Json.createArrayBuilder()// cria o array feature [][m
[32m+[m					[32m.add(Json.createObjectBuilder() // cria objeto {}[m
[32m+[m					[32m.add("type", "Feature")[m
[32m+[m					[32m.add("properties", Json.createObjectBuilder()[m
[32m+[m							[32m.add("id","1")[m
[32m+[m							[32m.add("name", "AAAA")[m
[32m+[m							[32m.add("description", "AAA"))[m
[32m+[m					[32m.add("geometry", Json.createObjectBuilder()[m
[32m+[m							[32m.add("type","Polygon")[m
[32m+[m							[32m.add("coordinates",Json.createArrayBuilder()[m
[32m+[m									[32m.add(Json.createArrayBuilder()[m
[32m+[m											[32m.add(Json.createArrayBuilder()[m
[32m+[m													[32m.add("La123,Lng321")[m
[32m+[m[41m													[m
