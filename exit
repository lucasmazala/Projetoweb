[32m+[m
[32m+[m	[32m@SuppressWarnings("unchecked")[m
[32m+[m	[32m/* Buscar √© usado para mexer com opera√ß√µes de excluir ou editar */[m
[32m+[m	[32mpublic Entity buscar(int codigo) {[m
[32m+[m		[32m// precisa abrir uma sessao[m
[32m+[m		[32mSession sessao = HibernateUtil.getFabricaDeSessoes().openSession();[m
[32m+[m		[32m// n√£o precisa de transa√ß√£o pq aqui s√≥ buscar, n√£o altera nem edita[m
[32m+[m		[32mtry {[m
[32m+[m			[32mCriteria consulta = sessao.createCriteria(classe);[m
[32m+[m			[32m/* idEq pega o valor do codigo passado ali em cima e compara com a[m
[32m+[m			[32m * chave prim√°ria da sua entidade[m
[32m+[m			[32m */[m
[32m+[m			[32mconsulta.add(Restrictions.idEq(codigo));[m
[32m+[m			[32m/* restri√ß√µes no crit√©ria(igual a cl√°usula where) */[m
[32m+[m
[32m+[m			[32m/* vari√°vel criada √© uma entidade. O uniqueResult serve para pegar[m
[32m+[m			[32m * apenas um resultado, e √© preciso fazer um casting, convers√£o para[m
[32m+[m			[32m * o tipo unidade[m
[32m+[m			[32m *  se for para pegar mais resultados usar um list.[m
[32m+[m			[32m */[m
[32m+[m			[32mEntity resultado = (Entity) consulta.uniqueResult();[m
[32m+[m			[32mreturn resultado;[m
[32m+[m		[32m} catch (RuntimeException erro) {[m
[32m+[m			[32mthrow erro;[m
[32m+[m		[32m} finally {[m
[32m+[m			[32msessao.close();[m
[32m+[m		[32m}[m
[32m+[m	[32m} //good[m
[32m+[m[41m		[m
[32m+[m	[32mpublic void excluir(Entity entidade) {[m
[32m+[m		[32m//muito parecido com o met√≥do de adicionar[m
[32m+[m		[32mSession sessao = HibernateUtil.getFabricaDeSessoes().openSession();[m
[32m+[m		[32mTransaction transacao = null;[m
[32m+[m		[32mtry {[m
[32m+[m			[32mtransacao = sessao.beginTransaction();[m
[32m+[m			[32msessao.delete(entidade);[m
[32m+[m			[32mtransacao.commit();[m
[32m+[m		[32m} catch (RuntimeException erro) {[m
[32m+[m
[32m+[m			[32mif (transacao != null) {[m
[32m+[m			[32m}[m
[32m+[m			[32mthrow erro;[m
[32m+[m		[32m} finally {[m[41m [m
[32m+[m			[32msessao.close();[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m[41m	[m
[32m+[m	[32mpublic void editar(Entity entidade) {[m
[32m+[m		[32m//muito parecido com o met√≥do de adicionar e excluir[m
[32m+[m		[32mSession sessao = HibernateUtil.getFabricaDeSessoes().openSession();[m
[32m+[m		[32mTransaction transacao = null;[m
[32m+[m		[32mtry {[m
[32m+[m			[32mtransacao = sessao.beginTransaction();[m
[32m+[m			[32msessao.update(entidade);[m
[32m+[m			[32mtransacao.commit();[m
[32m+[m		[32m} catch (RuntimeException erro) {[m
[32m+[m
[32m+[m			[32mif (transacao != null) {[m
[32m+[m			[32m}[m
[32m+[m			[32mthrow erro;[m
[32m+[m		[32m} finally {[m[41m [m
[32m+[m			[32msessao.close();[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
 }[m
